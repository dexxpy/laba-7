#include <iostream>
using namespace std;

int main() 
{
    setlocale(LC_ALL, "");
    int n = 10;
    int a = 0, b = 1, c;

    cout << "Первые 10 чисел Фибоначчи: " << endl;
    cout << a << " " << b;

    for (int i = 2; i < n; ++i) 
    {
        c = a + b;
        cout << " " << c;
        a = b;
        b = c;
    }

    return 0;
}
